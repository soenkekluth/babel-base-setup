{
  "name": "babel-base-setup",
  "version": "0.1.1",
  "description": "",
  "main": "./src/js/index.js",
  "scripts": {
    "test": "npm run dist && mocha test/dist.js",
    "test-babel": "mocha test/*.js --require babel-core/register",
    "test-build": "npm run build && mocha test/dist.js --require babel-core/register",
    "start": "npm run build && concurrently --kill-others \"npm run watch\" \"npm run server\"",
    "server": "browser-sync start --no-ghost-mode --server --directory --files 'examples/**/**, build/*.js' --startPath examples/index.html",
    "watch-js": "watchify --poll=300 --fast -e src/js/index.js -d -t babelify -o build/bundle.js -v",
    "watch": "npm run watch-js",
    "babel-watch": "babel --watch src/js/index.js -o build/bundle.js",
    "dev": "babel-node src/js/index.js",
    "build": "browserify --fast -e src/js/index.js -d -t babelify -o build/bundle.js && npm run build-lib",
    "build-lib": "babel src -d build",
    "dist": "BABEL_ENV=production browserify src/js/index.js -t babelify -o dist/bundle.js && npm run dist-lib",
    "dist-lib": "BABEL_ENV=production babel src -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soenkekluth/babel-base-setup.git"
  },
  "author": "SÃ¶nke Kluth <soenke.kluth@gmail.com> (http://soenkekluth.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/soenkekluth/babel-base-setup/issues"
  },
  "homepage": "https://github.com/soenkekluth/babel-base-setup#readme",
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-core": "6.10.4",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-class-constructor-call": "6.8.0",
    "babel-plugin-transform-class-properties": "6.10.2",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-es2015-classes": "6.9.0",
    "babel-plugin-transform-proto-to-assign": "6.9.0",
    "babel-plugin-transform-remove-console": "6.8.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-stage-0": "6.5.0",
    "babelify": "7.3.0",
    "browser-sync": "2.13.0",
    "browserify": "13.0.1",
    "concurrently": "2.2.0",
    "mocha": "2.5.3",
    "should": "9.0.2",
    "watchify": "3.7.0"
  }
}
